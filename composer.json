{
  "name": "fengjisheng/hyperf-hashing",
  "description": "The Hyperf Hashing package.",
  "keywords": [
    "php",
    "hyperf",
    "hashing"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "hyperf/config": "~3.1.0",
    "hyperf/di": "~3.1.0",
    "hyperf/support": "~3.1.0"
  },
  "autoload": {
    "psr-4": {
      "Hyperf\\Hashing\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "hyperf": {
      "config": "Hyperf\\Hashing\\ConfigProvider"
    },
    "branch-alias": {
      "dev-main": "3.1-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}